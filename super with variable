class Parent {
int num = 100;
}
class Child extends Parent {
int num = 200;
void display() {
System.out.println("Child num: " + num);
System.out.println("Parent num: " + super.num); // Access parent variable
}
}
public class Main {
public static void main(String[] args) {
Child c = new Child();
c.display();
}
}
